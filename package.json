{
  "name": "vuefes-2025",
  "private": true,
  "version": "2.6.0",
  "type": "module",
  "author": {
    "name": "ubugeeei",
    "email": "ubuge1122@gmail.com"
  },
  "packageManager": "pnpm@10.6.3",
  "engines": {
    "node": "22",
    "pnpm": ">=10"
  },
  "//": "NOTE: align to Netlify's Node.js version",
  "pnpm": {
    "executionEnv": {
      "nodeVersion": "22.14.0"
    },
    "overrides": {
      "vite": "npm:rolldown-vite@latest",
      "vue": "npm:vue@3.6.0-alpha.2"
    },
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "better-sqlite3",
      "esbuild",
      "sharp",
      "unrs-resolver",
      "workerd"
    ]
  },
  "scripts": {
    "build": "cross-env NODE_OPTIONS='--max-old-space-size=4096' nuxt build",
    "dev": "cross-env FEATURE_TIMETABLE=0 nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "check": "nuxi typecheck",
    "lint": "eslint . && textlint .",
    "lint:fix": "eslint --fix . && textlint --fix .",
    "spell": "cspell '**' --cache",
    "story:dev": "storybook dev -p 6006",
    "story:build": "storybook build",
    "peatix-api-gen": "pnpm dlx tsx scripts/peatix-api-gen",
    "release:major": "pnpm dlx changelogen@latest --release --major",
    "release:minor": "pnpm dlx changelogen@latest --release --minor",
    "release:patch": "pnpm dlx changelogen@latest --release --patch",
    "postinstall": "pnpm dlx tsx scripts/name-badge-admin-data-gen.ts && nuxt prepare"
  },
  "devDependencies": {
    "@auth/drizzle-adapter": "^1.10.0",
    "@aws-sdk/client-s3": "^3.842.0",
    "@aws-sdk/s3-request-presigner": "^3.842.0",
    "@chromatic-com/storybook": "^3",
    "@intlify/eslint-plugin-vue-i18n": "^4.0.1",
    "@nuxt/eslint": "1.9.0",
    "@nuxt/scripts": "0.11.2",
    "@nuxtjs/i18n": "9.4.0",
    "@nuxtjs/seo": "^3.0.1",
    "@nuxtjs/storybook": "^8.3.3",
    "@primevue/forms": "^4.3.2",
    "@primevue/nuxt-module": "^4.3.7",
    "@sidebase/nuxt-auth": "1.0.0",
    "@storybook-vue/nuxt": "^8.3.3",
    "@storybook/addon-essentials": "8.4.7",
    "@storybook/addon-interactions": "8.4.7",
    "@storybook/blocks": "8.4.7",
    "@storybook/test": "8.4.7",
    "@storybook/vue3": "8.4.7",
    "@types/three": "^0.175.0",
    "@unhead/vue": "^2.0.5",
    "@vueuse/core": "^13.2.0",
    "cross-env": "^7.0.3",
    "cspell": "^8.17.5",
    "d1-driver": "^2.0.2",
    "dotenv": "^17.0.1",
    "drizzle-kit": "^0.31.4",
    "drizzle-orm": "^0.44.4",
    "emoji-regex": "^10.4.0",
    "eslint": "^9.33.0",
    "eslint-plugin-storybook": "^9.0.7",
    "eslint-plugin-vuejs-accessibility": "^2.4.1",
    "lightningcss": "^1.30.1",
    "next-auth": "~4.21.1",
    "nitro": "npm:nitropack@^2.12.4",
    "nitro-cloudflare-dev": "^0.2.2",
    "nitropack": "^2.12.4",
    "node-fetch": "^3.3.2",
    "nuxt": "^4.0.3",
    "nuxt-og-image": "^5.1.9",
    "nuxt-typed-router": "3.8.0",
    "openapi-fetch": "^0.14.0",
    "openapi-typescript": "^7.8.0",
    "prettier": "4.0.0-alpha.11",
    "primevue": "^4.3.2",
    "rehype-external-links": "^3.0.0",
    "rehype-stringify": "^10.0.1",
    "remark-mdc": "^3.5.3",
    "remark-parse": "^11.0.0",
    "remark-rehype": "^11.1.2",
    "resolvers": "link:@primevue/forms/resolvers",
    "satori": "^0.16.2",
    "storybook": "8.4.7",
    "textlint": "^14.4.2",
    "textlint-filter-rule-allowlist": "^4.0.0",
    "textlint-rule-preset-vuejs-jp": "github:vuejs-jp/textlint-rule-preset-vuejs-jp",
    "three": "^0.178.0",
    "typescript": "5.8.3",
    "unified": "^11.0.5",
    "unplugin-icons": "^22.1.0",
    "unstorage": "^1.16.1",
    "valibot": "^1.1.0",
    "vue": "latest",
    "vue-router": "latest",
    "vue-tsc": "^2.2.8",
    "wrangler": "^4.23.0"
  },
  "eslintConfig": {
    "extends": [
      "plugin:storybook/recommended"
    ]
  }
}
