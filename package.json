{
  "name": "vuefes-2025",
  "private": true,
  "version": "1.1.0",
  "type": "module",
  "author": {
    "name": "ubugeeei",
    "email": "ubuge1122@gmail.com"
  },
  "packageManager": "pnpm@10.6.3",
  "engines": {
    "node": "22",
    "pnpm": ">=10"
  },
  "//": "NOTE: align to Netlify's Node.js version",
  "pnpm": {
    "executionEnv": {
      "nodeVersion": "22.14.0"
    },
    "onlyBuiltDependencies": [
      "better-sqlite3",
      "esbuild",
      "@parcel/watcher"
    ]
  },
  "scripts": {
    "build": "nuxt build",
    "dev": "FEATURE_CFP=1 FEATURE_TICKET=0 FEATURE_SPONSOR_LIST=0 nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "check": "nuxi typecheck",
    "lint": "eslint . && textlint .",
    "lint:fix": "eslint --fix . && textlint --fix .",
    "spell": "cspell '**' --cache",
    "story:dev": "storybook dev -p 6006",
    "story:build": "storybook build",
    "release:major": "pnpx changelogen@latest --release --major",
    "release:minor": "pnpx changelogen@latest --release --minor",
    "release:patch": "pnpx changelogen@latest --release --patch",
    "postinstall": "nuxt prepare"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^3",
    "@intlify/eslint-plugin-vue-i18n": "^4.0.1",
    "@nuxt/eslint": "1.3.0",
    "@nuxt/scripts": "0.11.2",
    "@nuxtjs/i18n": "9.4.0",
    "@nuxtjs/seo": "^3.0.1",
    "@nuxtjs/storybook": "^8.3.3",
    "@primevue/forms": "^4.3.2",
    "@primevue/nuxt-module": "^4.3.2",
    "@storybook-vue/nuxt": "^8.3.3",
    "@storybook/addon-essentials": "8.4.7",
    "@storybook/addon-interactions": "8.4.7",
    "@storybook/blocks": "8.4.7",
    "@storybook/test": "8.4.7",
    "@storybook/vue3": "8.4.7",
    "@types/three": "^0.175.0",
    "@unhead/vue": "^2.0.5",
    "@vueuse/core": "^13.2.0",
    "cspell": "^8.17.5",
    "eslint": "^9.24.0",
    "eslint-plugin-storybook": "^0.12.0",
    "eslint-plugin-vuejs-accessibility": "^2.4.1",
    "lightningcss": "^1.29.1",
    "nuxt": "^3.15.4",
    "prettier": "4.0.0-alpha.11",
    "primevue": "^4.3.2",
    "rehype-external-links": "^3.0.0",
    "rehype-stringify": "^10.0.1",
    "remark-mdc": "^3.5.3",
    "remark-parse": "^11.0.0",
    "remark-rehype": "^11.1.2",
    "resolvers": "link:@primevue/forms/resolvers",
    "storybook": "8.4.7",
    "textlint": "^14.4.2",
    "textlint-filter-rule-allowlist": "^4.0.0",
    "textlint-rule-preset-vuejs-jp": "github:vuejs-jp/textlint-rule-preset-vuejs-jp",
    "three": "^0.175.0",
    "typescript": "5.8.3",
    "unified": "^11.0.5",
    "unplugin-icons": "^22.1.0",
    "vue": "latest",
    "vue-router": "latest",
    "vue-tsc": "^2.2.8",
    "zod": "^3.24.2"
